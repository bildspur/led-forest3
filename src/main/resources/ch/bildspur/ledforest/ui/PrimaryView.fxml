<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="600.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <AnchorPane prefHeight="60.0" prefWidth="200.0" style="-fx-background-color: #333333;"
                    BorderPane.alignment="CENTER">
            <ProgressIndicator fx:id="progressIndicator" layoutX="561.0" layoutY="18.0" prefHeight="25.0"
                               prefWidth="25.0" AnchorPane.rightAnchor="14.0"/>
            <FlowPane hgap="5.0" layoutY="-2.0" prefHeight="60.0" prefWidth="750.0" vgap="5.0"
                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="0.0">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <Button mnemonicParsing="false" onAction="#newProject" text="New Project"/>
                <Button mnemonicParsing="false" onAction="#loadProject" text="Load Project"/>
                <Button mnemonicParsing="false" onAction="#saveProject" text="Save Project"/>
                <Separator orientation="VERTICAL"/>
            </FlowPane>
        </AnchorPane>
    </top>
    <left>
        <VBox prefHeight="500.0" prefWidth="260.0" BorderPane.alignment="CENTER">
            <Label text="Scene">
                <VBox.margin>
                    <Insets bottom="5.0" left="5.0" top="5.0"/>
                </VBox.margin>
            </Label>
            <TreeView fx:id="elementTreeView" prefWidth="260.0"/>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="10.0" top="10.0"/>
                </padding>
                <Button mnemonicParsing="false" onAction="#addTube" text="Add"/>
                <Button mnemonicParsing="false" onAction="#removeTube" text="Remove"/>
            </HBox>
        </VBox>
    </left>
    <bottom>
        <FlowPane prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #696969;"
                  BorderPane.alignment="CENTER">
            <Label fx:id="statusLabel" text="Status" textFill="WHITE">
                <FlowPane.margin>
                    <Insets left="10.0" top="10.0"/>
                </FlowPane.margin>
            </Label>
        </FlowPane>
    </bottom>
    <center>
    </center>
    <right>
    </right>
</BorderPane>